{
  "name": "@baseverify/sdk-js",
  "version": "0.0.1",
  "description": "Base Verify javascript sdk",
  "main": "dist/sdk-js.min.js",
  "types": "dist/sdk-js.d.ts",
  "scripts": {
    "start": "concurrently --kill-others \"tsc -w\" \"nodemon --delay 1 dist/index.js\"",
    "compile": "tsc && node dist/index.js",
    "dev": "nodemon -e ts --exec \"yarn compile\"",
    "_clear": "rimraf compiled/* dist/*",
    "_tsc": "tsc",
    "_make-bundle": "node make-bundle",
    "build": "run-s _clear _tsc _make-bundle"
  },
  "repository": "https://github.com/baseverify/bv-sdk-js.git",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
